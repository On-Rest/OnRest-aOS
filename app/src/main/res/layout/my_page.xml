<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background">
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_logo_start"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.036" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_logo_top"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.005" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_logo_end"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.2167" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_logo_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.05" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_profile_top"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.08" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_profile_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1925" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_profile_start"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.2944" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_profile_end"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.53" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_nickname_top"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.12" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_nickname_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1481" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_nickname_start"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5689" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_nickname_end"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.7056" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_separator_top"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2175" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_separator_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2225" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_separator_start"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.0444" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_separator_end"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9556" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MA_top"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2775" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MA_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.3013" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MA_start"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.1222" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MA_end"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.4639" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MAbackground_top"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.2475" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MAbackground_bottom"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5638" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MAbackground_start"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.0444" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/g_MAbackground_end"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9556" />
        <View
            android:id="@+id/separator"
            android:layout_width="328dp"
            android:layout_height="4dp"
            android:background="#EAE9E7"
            app:layout_constraintTop_toTopOf="@id/g_separator_top"
            app:layout_constraintBottom_toBottomOf="@id/g_separator_bottom"
            app:layout_constraintEnd_toEndOf="@id/g_separator_end"
            app:layout_constraintStart_toStartOf="@id/g_separator_start"
            />
        <ImageButton
            android:id="@+id/wholerest"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/wholerest"
            app:layout_constraintBottom_toBottomOf="@id/g_logo_bottom"
            app:layout_constraintEnd_toEndOf="@id/g_logo_end"
            app:layout_constraintStart_toStartOf="@id/g_logo_start"
            app:layout_constraintTop_toTopOf="@id/g_logo_top"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/profile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:src="@drawable/profile"
            app:layout_constraintBottom_toBottomOf="@id/g_profile_bottom"
            app:layout_constraintEnd_toEndOf="@id/g_profile_end"
            app:layout_constraintStart_toStartOf="@id/g_profile_start"
            app:layout_constraintTop_toTopOf="@id/g_profile_top" />

        <TextView
            android:id="@+id/nickname"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="홍길동"
            android:textStyle="bold"
            android:gravity="top|center"
            app:layout_constraintBottom_toBottomOf="@id/g_nickname_bottom"
            app:layout_constraintEnd_toEndOf="@id/g_nickname_end"
            app:layout_constraintStart_toStartOf="@id/g_nickname_start"
            app:layout_constraintTop_toTopOf="@id/g_nickname_top" />

        <View
            android:id="@+id/month_analysis_background"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@drawable/analysis_background"
            app:layout_constraintTop_toTopOf="@id/g_MAbackground_top"
            app:layout_constraintBottom_toBottomOf="@id/g_MAbackground_bottom"
            app:layout_constraintStart_toStartOf="@id/g_MAbackground_start"
            app:layout_constraintEnd_toEndOf="@id/g_MAbackground_end"/>
        <TextView
            android:id="@+id/month_analysis"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="이번 달의 기분 분포"
            app:layout_constraintTop_toTopOf="@id/g_MA_top"
            app:layout_constraintBottom_toBottomOf="@id/g_MA_bottom"
            app:layout_constraintStart_toStartOf="@id/g_MA_start"
            app:layout_constraintEnd_toEndOf="@id/g_MA_end"
            android:gravity="top|center"
            android:textStyle="bold"
            />

</androidx.constraintlayout.widget.ConstraintLayout>
