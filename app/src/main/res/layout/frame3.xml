<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#F8F5F1">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/highhill"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent">
    </ImageView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical0.5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5">
        </androidx.constraintlayout.widget.Guideline>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.045">
        </androidx.constraintlayout.widget.Guideline>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.175">
        </androidx.constraintlayout.widget.Guideline>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.305">
        </androidx.constraintlayout.widget.Guideline>


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.435">
        </androidx.constraintlayout.widget.Guideline>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.565">
        </androidx.constraintlayout.widget.Guideline>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.695">
        </androidx.constraintlayout.widget.Guideline>


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.825">
        </androidx.constraintlayout.widget.Guideline>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/vertical8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.955">
        </androidx.constraintlayout.widget.Guideline>


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.23">
        </androidx.constraintlayout.widget.Guideline>

        <ImageView
            android:id="@+id/wholerest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/wholerest">
        </ImageView>

        <ImageView
            android:id="@+id/square"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/quarter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/progress"
            app:layout_constraintBottom_toBottomOf="@+id/progress">
        </ImageView>

        <ImageButton
            android:id="@+id/speechbubble"
            android:layout_width="184dp"
            android:layout_height="41dp"
            android:background="none"
            android:src="@drawable/choiceemo"
            app:layout_constraintBottom_toTopOf="@id/rectangle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="3dp">
        </ImageButton>

        <ImageButton
            android:id="@+id/stripe"
            android:layout_width="52dp"
            android:layout_height="78dp"
            android:src="@drawable/stripe"
            android:scaleType="fitXY"
            android:background="none"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/box">
        </ImageButton>

        <ImageView
            android:id="@+id/rectangle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/rectangle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/stripe"
            android:layout_marginBottom="8dp"
            tools:ignore="DuplicateIds">
        </ImageView>


        <TextView
            android:id="@+id/box"
            android:layout_width="10dp"
            android:layout_height="130dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">
        </TextView>

        <TextView
            android:id="@+id/roundsquaer"
            android:layout_width="280dp"
            android:layout_height="90dp"
            android:src="@drawable/frame2square"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="28dp"
            android:background="@drawable/rountradius">
        </TextView>

        <ImageView
            android:id="@+id/happyface"
            android:layout_width="59dp"
            android:layout_height="50dp"
            android:src="@drawable/happyface"
            app:layout_constraintStart_toStartOf="@id/roundsquaer"
            app:layout_constraintTop_toTopOf="@id/roundsquaer"
            app:layout_constraintBottom_toBottomOf="@id/roundsquaer"
            app:layout_constraintEnd_toStartOf="@id/angryface"
            android:layout_marginStart="15dp">
        </ImageView>

        <ImageView
            android:id="@+id/angryface"
            android:layout_width="59dp"
            android:layout_height="50dp"
            android:src="@drawable/angryface"
            app:layout_constraintTop_toTopOf="@id/roundsquaer"
            app:layout_constraintBottom_toBottomOf="@id/roundsquaer"
            app:layout_constraintEnd_toStartOf="@id/sadface"
            app:layout_constraintStart_toEndOf="@id/happyface">
        </ImageView>

        <ImageView
            android:id="@+id/sadface"
            android:layout_width="59dp"
            android:layout_height="50dp"
            android:src="@drawable/sadface"
            app:layout_constraintBottom_toBottomOf="@id/roundsquaer"
            app:layout_constraintEnd_toEndOf="@id/roundsquaer"
            app:layout_constraintStart_toStartOf="@+id/vertical5"
            app:layout_constraintTop_toTopOf="@id/roundsquaer"
            android:layout_marginEnd="15dp">
        </ImageView>


    </androidx.constraintlayout.widget.ConstraintLayout>





</androidx.constraintlayout.widget.ConstraintLayout>